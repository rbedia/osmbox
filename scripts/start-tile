data_dir=/home/rafael/src/osmbox/data
mkdir -p $data_dir/styles
sudo /usr/bin/docker run -d \
    --name osmbox_tile \
    --link osmbox_db:db \
	-e POSTGRES_USER=powerpy \
	-e POSTGRES_PASSWORD=randomword \
	-e POSTGRES_DB=powerpy \
	-v ${data_dir}/styles:/opt/styles \
	-p 8112:80 \
	osmbox-tile:latest

